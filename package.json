		"js-yaml": "^4.1.0",
		"lodash": "^4.17.21",
		"node-emoji": "^1.11.0",
		"promise-retry": "^2.0.1",
		"snuze": "^1.0.0",
		"split": "^1.0.0",
		"stringify-object": "^3.3.0",
		"winston": "^3.13.0"
	},
	"devDependencies": {
		"eslint": "^8.57.0",
		"eslint-config-eslint": "^9.0.0",
		"jest": "^20.0.4",
		"publish": "^0.6.0"
	},
	"eslintConfig": {
		"extends": "eslint",
		"env": {
			"node": true,
			"es6": true
		},
		"rules": {
			"quotes": [
				"error",
				"single"
			],
			"indent": [
				"error",
				2
			],
			"require-jsdoc": "off",
			"brace-style": [
				"error",
				"stroustrup"
			]
		}
	},
	"engines": {
		"node": ">=6.0.0"
	}
}
