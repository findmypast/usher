	"devDependencies": {
		"eslint": "^4.19.1",
		"eslint-config-eslint": "^4.0.0",
		"jest": "^29.3.1",
		"publish": "^0.6.0"
	},
	"eslintConfig": {
		"extends": "eslint",
		"env": {
			"node": true,
			"es6": true
		},
		"rules": {
			"quotes": [
				"error",
				"single"
			],
			"indent": [
				"error",
				2
			],
			"require-jsdoc": "off",
			"brace-style": [
				"error",
				"stroustrup"
			]
		}
	},
	"engines": {
		"node": ">=6.0.0"
	}
}
